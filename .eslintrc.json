{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "jsx-quotes": [2, "prefer-single"],
    "implicit-arrow-linebreak": ["error", "beside"],
    "no-param-reassign": 0
    // "arrow-body-style": ["error", "as-needed"]
  }
}
