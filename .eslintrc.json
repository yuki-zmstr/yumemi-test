{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "jsx-quotes": [2, "prefer-single"],
    "implicit-arrow-linebreak": ["error", "beside"],
    "no-param-reassign": 0,
    "prettier/prettier": [
      "error",
      { "singleQuote": true, "jsxSingleQuote": true, "printWidth": 100 }
    ]
  }
}
